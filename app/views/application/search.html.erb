<div class="col-md-12 well">           
    <div class="row">
        <form class="col-md-offset-3 col-md-6" method="get" action="/submit.html/">
            <div class="input-group">
                <input name="query" type="text" class="form-control" placeholder="Search String..." value="<%= @query %>"/>
                <span class="input-group-btn">
                  <button type="submit" class="btn btn-primary"><i class="glyphicon glyphicon-search"></i>Go</button>
                </span>
            </div>
        </form>
    </div>
    
    <hr>

<% 0.upto(@output.length - 1) do |i| %>
  <div class="row">
    <div class="col-md-1"><img src="http://cdn.stoychev.info/icons/index/<%= @output[i].split('.')[-1].downcase %>.png" class="img-responsive" alt="<%= @output[i].split('.')[-1].downcase %>" /></div>
    <div class="col-md-11">
      <%  0.upto(@output[i].split('/').length - 2).each do | n | %>
            <span class="divider muted">/ </span> 
            <a href="/browse.html/<%= @output_urlencoded[i].split('/')[0..n].join('/') %>/"><%= @output[i].split('/')[n] %></a>
      <% end %>
            <span class="divider muted">/ </span> 
            <a href="/music/<%= @output_urlencoded[i] %>"><%= @output[i].split('/')[-1] %></a>
    </div>
  </div>
<% end %>
</div>
